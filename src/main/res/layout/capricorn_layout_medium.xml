<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/fl_Capricorn"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:background="@drawable/background_transp_dk"
    android:baselineAligned="false"
	android:clickable="false"
	android:focusable="false"
	android:orientation="horizontal"
	android:paddingBottom="0dp"
	android:paddingLeft="4dp"
	android:paddingRight="4dp"
	android:paddingTop="0dp" >
	
    <LinearLayout
        android:id="@+id/ll_1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:clickable="false"
        android:focusable="false"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/ll_tv_1"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="3"
            android:clickable="false"
            android:focusable="false"
            android:gravity="bottom"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/tv_1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:gravity="center"
                android:text="@string/DefaultText"
                android:textColor="@android:color/white" />

            <TextView
                android:id="@+id/tv_1_unit"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:gravity="center"
                android:text="@string/DontKnow"
                android:textColor="@android:color/white" />

        </LinearLayout>

        <ImageView
            android:id="@+id/iv_1"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="4"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/capricorn" />

    </LinearLayout>

    <LinearLayout
    	android:id="@+id/ll_2"
    	android:layout_width="fill_parent"
    	android:layout_height="fill_parent"
    	android:layout_weight="1"
    	android:clickable="false"
    	android:focusable="false"
    	android:orientation="vertical" >

        <ImageView
            android:id="@+id/iv_2"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="4"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/capricorn" />

    <LinearLayout
        android:id="@+id/ll_tv_2"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_gravity="center"
        android:layout_marginBottom="2dp"
        android:layout_weight="3"
        android:clickable="false"
        android:focusable="false"
        android:gravity="top"
        android:orientation="vertical" >
        
        <TextView
            android:id="@+id/tv_2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="top|center"
            android:text="@string/DefaultText"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/tv_2_unit"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="top|center"
            android:text="@string/DontKnow"
            android:textColor="@android:color/white" />

        </LinearLayout>
        
    </LinearLayout>

    <LinearLayout
    	android:id="@+id/ll_3"
    	android:layout_width="fill_parent"
    	android:layout_height="fill_parent"
    	android:layout_weight="1"
    	android:clickable="false"
    	android:focusable="false"
    	android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/ll_tv_3"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_gravity="center"
        android:layout_weight="3"
        android:clickable="false"
        android:focusable="false"
        android:gravity="bottom"
        android:orientation="vertical" >
        
        <TextView
            android:id="@+id/tv_3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:text="@string/DefaultText"
            android:textColor="@android:color/white" />
        
        <TextView
            android:id="@+id/tv_3_unit"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:text="@string/DontKnow"
            android:textColor="@android:color/white" />

        </LinearLayout>

        <ImageView
            android:id="@+id/iv_3"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="4"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/capricorn" />

    </LinearLayout>
    
    <LinearLayout
    	android:id="@+id/ll_4"
    	android:layout_width="fill_parent"
    	android:layout_height="fill_parent"
    	android:layout_weight="1"
    	android:clickable="false"
    	android:focusable="false"
    	android:orientation="vertical" >

        <ImageView
        	android:id="@+id/iv_4"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="4"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/capricorn" />

    <LinearLayout
    	android:id="@+id/ll_tv_4"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_gravity="center"
        android:layout_marginBottom="2dp"
        android:layout_weight="3"
        android:clickable="false"
        android:focusable="false"
        android:gravity="top"
        android:orientation="vertical" >
        
        <TextView
        	android:id="@+id/tv_4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:text="@string/DefaultText"
            android:textColor="@android:color/white" />

        <TextView
        	android:id="@+id/tv_4_unit"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:text="@string/DontKnow"
            android:textColor="@android:color/white" />

        </LinearLayout>
        
    </LinearLayout>

    <LinearLayout
    	android:id="@+id/ll_5"
    	android:layout_width="fill_parent"
    	android:layout_height="fill_parent"
    	android:layout_weight="1"
    	android:clickable="false"
    	android:focusable="false"
    	android:orientation="vertical" >

    <LinearLayout
    	android:id="@+id/ll_tv_5"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_gravity="center"
        android:layout_weight="3"
        android:clickable="false"
        android:focusable="false"
        android:gravity="bottom"
        android:orientation="vertical" >
        
        <TextView
        	android:id="@+id/tv_5"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:text="@string/DefaultText"
            android:textColor="@android:color/white" />
        
        <TextView
        	android:id="@+id/tv_5_unit"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:text="@string/DontKnow"
            android:textColor="@android:color/white" />

        </LinearLayout>

        <ImageView
        	android:id="@+id/iv_5"
	        android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="4"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/capricorn" />

    </LinearLayout>
        

    
</LinearLayout>